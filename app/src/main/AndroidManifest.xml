<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bassem.clinic_userapp">

    <application
        android:allowBackup="true"
        android:icon="@drawable/userlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/userlogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Clinicuserapp">
        <activity
            android:name=".log.MainActivity"
            android:exported="false" />
        <activity
            android:name=".ui.Container"
            android:exported="false" />
        <activity
            android:name=".log.SplashActvity"
            android:exported="true"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>